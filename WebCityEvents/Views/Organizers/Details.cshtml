@model WebCityEvents.ViewModels.OrganizerViewModel

@{
    ViewData["Title"] = "Детали";
}

<h1>Детали</h1>

<div>
    <h4>Организатор</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <strong>ФИО</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FullName)
        </dd>
        <dt class="col-sm-2">
            <strong>Должность</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Post)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.OrganizerID">Редактировать</a> |
    <a asp-action="Index">Назад к списку</a>
</div>