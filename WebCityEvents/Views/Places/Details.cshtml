@model WebCityEvents.ViewModels.PlaceViewModel

@{
    ViewData["Title"] = "Детали";
}

<h1>Детали</h1>

<div>
    <h4>Место</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <strong>Название места</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PlaceName)
        </dd>
        <dt class="col-sm-2">
            <strong>Геолокация</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Geolocation)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.PlaceID">Редактировать</a> |
    <a asp-action="Index">Назад к списку</a>
</div>